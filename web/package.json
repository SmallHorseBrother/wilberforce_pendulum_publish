{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/aliyun-theme": "^0.0.4",
    "@ant-design/pro-card": "^1.10.4",
    "@ant-design/pro-form": "1.39",
    "@ant-design/pro-layout": "^6.14.0",
    "@umijs/preset-react": "1.x",
    "ali-oss": "^6.15.2",
    "antd": "^4.12.3",
    "axios": "^0.24.0",
    "clipboard": "^2.0.8",
    "copy-to-clipboard": "^3.3.1",
    "crypto-js": "^4.0.0",
    "echarts": "^5.2.0",
    "file-loader": "^6.2.0",
    "react-clipboardjs-copy": "^1.3.2",
    "react-latex": "^2.0.0",
    "react-markdown": "^5.0.3",
    "remark-gfm": "^1.0.0",
    "three": "^0.132.2",
    "umi": "^3.3.7",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.3.7",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
